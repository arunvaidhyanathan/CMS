      {
        "name": "03. Case Creation",
        "item": [
          {
            "name": "Create Harassment Case (Single Dept)",
            "event": [
              {
                "listen": "test",
                "script": {
                  "exec": [
                    "if (pm.response.code === 201) {",
                    "    const response = pm.response.json();",
                    "    pm.collectionVariables.set('case1Id', response.caseId);",
                    "    pm.test('Case created successfully', () => {",
                    "        pm.expect(response.caseNumber).to.include('CMS-');",
                    "    });",
                    "}"
                  ]
                }
              }
            ],
            "request": {
              "method": "POST",
              "header": [
                {
                  "key": "Authorization",
                  "value": "Bearer {{intakeToken}}"
                },
                {
                  "key": "Content-Type",
                  "value": "application/json"
                }
              ],
              "body": {
                "mode": "raw",
                "raw": "{\n    \"title\": \"Workplace Harassment Complaint\",\n    \"description\": \"Employee reports inappropriate behavior from supervisor during team meetings\",\n    \"caseTypeCode\": \"HARASSMENT\",\n    \"allegationType\": \"Sexual Harassment\",\n    \"severity\": \"HIGH\",\n    \"priority\": \"HIGH\",\n    \"complainantName\": \"Jane Smith\",\n    \"complainantEmail\": \"jane.smith@company.com\"\n}"
              },
              "url": {
