      </bpmndi:BPMNEdge>
      
      <!-- Sequence Flows for Pool 2 -->
      <bpmndi:BPMNEdge id="Flow_to_Gateway_Classification_di" bpmnElement="Flow_to_Gateway_Classification">
        <di:waypoint x="248" y="450" />
        <di:waypoint x="315" y="450" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_to_HR_di" bpmnElement="Flow_to_HR">
        <di:waypoint x="340" y="425" />
        <di:waypoint x="340" y="390" />
        <di:waypoint x="430" y="390" />
        <bpmndi:BPMNLabel>
