      <bpmndi:BPMNShape id="Participant_Ethics_Closure_di" bpmnElement="Participant_Ethics_Closure" isHorizontal="true">
        <dc:Bounds x="160" y="1140" width="600" height="200" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      
