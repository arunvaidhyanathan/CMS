      <bpmndi:BPMNShape id="StartEvent_Case_Created_di" bpmnElement="StartEvent_Case_Created">
        <dc:Bounds x="212" y="162" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="196" y="205" width="68" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_EO_Intake_di" bpmnElement="Task_EO_Intake">
        <dc:Bounds x="320" y="140" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Classification_DMN_di" bpmnElement="Task_Classification_DMN">
        <dc:Bounds x="490" y="140" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="EndEvent_EO_Process_di" bpmnElement="EndEvent_EO_Process">
        <dc:Bounds x="652" y="162" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="632" y="205" width="76" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Process Elements for Pool 2: Specialized Departments -->
      <bpmndi:BPMNShape id="StartEvent_Specialized_di" bpmnElement="StartEvent_Specialized">
        <dc:Bounds x="212" y="432" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="188" y="475" width="84" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Gateway_Classification_di" bpmnElement="Gateway_Classification" isMarkerVisible="true">
        <dc:Bounds x="315" y="425" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="298" y="482" width="84" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_HR_Assignment_di" bpmnElement="Task_HR_Assignment">
        <dc:Bounds x="430" y="350" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Legal_Assignment_di" bpmnElement="Task_Legal_Assignment">
        <dc:Bounds x="430" y="450" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_CSIS_Assignment_di" bpmnElement="Task_CSIS_Assignment">
        <dc:Bounds x="430" y="550" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Gateway_Converge_di" bpmnElement="Gateway_Converge" isMarkerVisible="true">
        <dc:Bounds x="605" y="425" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="585" y="482" width="90" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="EndEvent_Specialized_di" bpmnElement="EndEvent_Specialized">
        <dc:Bounds x="722" y="432" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="697" y="475" width="86" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Message Flows -->
      <bpmndi:BPMNEdge id="Flow_EO_to_Specialized_di" bpmnElement="Flow_EO_to_Specialized">
        <di:waypoint x="540" y="220" />
        <di:waypoint x="540" y="280" />
        <di:waypoint x="230" y="280" />
        <di:waypoint x="230" y="432" />
