<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:zeebe="http://camunda.org/schema/zeebe/1.0" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_1o1wpws" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.35.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.5.0">
  
  <bpmn:collaboration id="Collaboration_CMS_Process">
    
    <!-- Pool 1: Ethics Office - Intake and Classification -->
    <bpmn:participant id="Participant_Ethics_Office" name="Ethics Office - Intake &amp; Classification" processRef="Process_Ethics_Office" />
    
    <!-- Pool 2: Specialized Departments (HR, Legal, CSIS) -->
    <bpmn:participant id="Participant_Specialized_Depts" name="Specialized Departments (HR / Legal / CSIS)" processRef="Process_Specialized_Departments" />
    
    <!-- Pool 3: Investigation Unit - Management -->
    <bpmn:participant id="Participant_IU_Management" name="Investigation Unit - Management" processRef="Process_IU_Management" />
    
    <!-- Pool 4: Investigation Unit - Investigation Team -->
    <bpmn:participant id="Participant_IU_Investigation" name="Investigation Unit - Investigation Team" processRef="Process_IU_Investigation" />
    
    <!-- Pool 5: Ethics Office - Case Closure -->
    <bpmn:participant id="Participant_Ethics_Closure" name="Ethics Office - Case Closure &amp; Oversight" processRef="Process_Ethics_Closure" />
    
    <!-- Message Flows between pools -->
    <bpmn:messageFlow id="Flow_EO_to_Specialized" sourceRef="Task_Classification_DMN" targetRef="StartEvent_Specialized" />
    <bpmn:messageFlow id="Flow_Specialized_to_IU_Mgmt" sourceRef="EndEvent_Specialized" targetRef="StartEvent_IU_Management" />
    <bpmn:messageFlow id="Flow_IU_Mgmt_to_Investigation" sourceRef="Task_IU_Assignment" targetRef="StartEvent_Investigation" />
    <bpmn:messageFlow id="Flow_Investigation_to_Closure" sourceRef="Task_Investigation_Finalization" targetRef="StartEvent_Closure" />
    
  </bpmn:collaboration>

  <!-- Process 1: Ethics Office - Intake & Classification -->
  <bpmn:process id="Process_Ethics_Office" name="Ethics Office Intake Process" isExecutable="true">
    
    <bpmn:startEvent id="StartEvent_Case_Created" name="Case Created">
      <bpmn:outgoing>Flow_to_EO_Intake</bpmn:outgoing>
    </bpmn:startEvent>
    
    <bpmn:userTask id="Task_EO_Intake" name="EO Intake - Intake Process">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="INTAKE_ANALYST_GROUP" />
        <zeebe:formDefinition externalReference="eoIntakeForm" />
        <zeebe:userTask />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_to_EO_Intake</bpmn:incoming>
      <bpmn:outgoing>Flow_to_Classification</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:businessRuleTask id="Task_Classification_DMN" name="Classify Allegations">
      <bpmn:extensionElements>
        <zeebe:calledDecision decisionId="allegation-classification-dmn" resultVariable="classification" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_to_Classification</bpmn:incoming>
      <bpmn:outgoing>Flow_to_End_EO</bpmn:outgoing>
    </bpmn:businessRuleTask>
    
    <bpmn:endEvent id="EndEvent_EO_Process" name="Case Classified">
      <bpmn:incoming>Flow_to_End_EO</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows -->
    <bpmn:sequenceFlow id="Flow_to_EO_Intake" sourceRef="StartEvent_Case_Created" targetRef="Task_EO_Intake" />
    <bpmn:sequenceFlow id="Flow_to_Classification" sourceRef="Task_EO_Intake" targetRef="Task_Classification_DMN" />
    <bpmn:sequenceFlow id="Flow_to_End_EO" sourceRef="Task_Classification_DMN" targetRef="EndEvent_EO_Process" />
    
  </bpmn:process>

  <!-- Process 2: Specialized Departments (HR, Legal, CSIS) -->
  <bpmn:process id="Process_Specialized_Departments" name="Specialized Department Assignment" isExecutable="false">
    
    <bpmn:startEvent id="StartEvent_Specialized" name="Case Received for Assignment">
      <bpmn:outgoing>Flow_to_Gateway_Classification</bpmn:outgoing>
    </bpmn:startEvent>
    
    <bpmn:exclusiveGateway id="Gateway_Classification" name="Route by Classification">
      <bpmn:incoming>Flow_to_Gateway_Classification</bpmn:incoming>
      <bpmn:outgoing>Flow_to_HR</bpmn:outgoing>
      <bpmn:outgoing>Flow_to_Legal</bpmn:outgoing>
      <bpmn:outgoing>Flow_to_CSIS</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:userTask id="Task_HR_Assignment" name="EO Assignment - HR">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="HR_GROUP" />
        <zeebe:formDefinition externalReference="hrAssignmentForm" />
        <zeebe:userTask />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_to_HR</bpmn:incoming>
      <bpmn:outgoing>Flow_HR_to_Converge</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:userTask id="Task_Legal_Assignment" name="EO Assignment - Legal">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="LEGAL_GROUP" />
        <zeebe:formDefinition externalReference="legalAssignmentForm" />
        <zeebe:userTask />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_to_Legal</bpmn:incoming>
      <bpmn:outgoing>Flow_Legal_to_Converge</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:userTask id="Task_CSIS_Assignment" name="EO Assignment - CSIS">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="CSIS_GROUP" />
        <zeebe:formDefinition externalReference="csisAssignmentForm" />
        <zeebe:userTask />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_to_CSIS</bpmn:incoming>
      <bpmn:outgoing>Flow_CSIS_to_Converge</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:exclusiveGateway id="Gateway_Converge" name="Converge Assignments">
      <bpmn:incoming>Flow_HR_to_Converge</bpmn:incoming>
      <bpmn:incoming>Flow_Legal_to_Converge</bpmn:incoming>
      <bpmn:incoming>Flow_CSIS_to_Converge</bpmn:incoming>
      <bpmn:outgoing>Flow_to_End_Specialized</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:endEvent id="EndEvent_Specialized" name="Assignment Complete">
      <bpmn:incoming>Flow_to_End_Specialized</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows -->
    <bpmn:sequenceFlow id="Flow_to_Gateway_Classification" sourceRef="StartEvent_Specialized" targetRef="Gateway_Classification" />
    
    <bpmn:sequenceFlow id="Flow_to_HR" name="HR Cases" sourceRef="Gateway_Classification" targetRef="Task_HR_Assignment">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=classification = "HR"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_to_Legal" name="Legal Cases" sourceRef="Gateway_Classification" targetRef="Task_Legal_Assignment">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=classification = "LEGAL"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_to_CSIS" name="CSIS Cases" sourceRef="Gateway_Classification" targetRef="Task_CSIS_Assignment">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=classification = "CSIS"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_HR_to_Converge" sourceRef="Task_HR_Assignment" targetRef="Gateway_Converge" />
    <bpmn:sequenceFlow id="Flow_Legal_to_Converge" sourceRef="Task_Legal_Assignment" targetRef="Gateway_Converge" />
    <bpmn:sequenceFlow id="Flow_CSIS_to_Converge" sourceRef="Task_CSIS_Assignment" targetRef="Gateway_Converge" />
    <bpmn:sequenceFlow id="Flow_to_End_Specialized" sourceRef="Gateway_Converge" targetRef="EndEvent_Specialized" />
    
  </bpmn:process>

  <!-- Process 3: Investigation Unit - Management -->
  <bpmn:process id="Process_IU_Management" name="Investigation Unit Management" isExecutable="false">
    
    <bpmn:startEvent id="StartEvent_IU_Management" name="Case Received for IU Processing">
      <bpmn:outgoing>Flow_to_IU_Intake_Assignment</bpmn:outgoing>
    </bpmn:startEvent>
    
    <bpmn:userTask id="Task_IU_Intake_Assignment" name="IU Intake - Assignment">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="IU_MANAGER_GROUP" />
        <zeebe:formDefinition externalReference="iuIntakeAssignmentForm" />
        <zeebe:userTask />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_to_IU_Intake_Assignment</bpmn:incoming>
      <bpmn:outgoing>Flow_to_IU_Processing</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:userTask id="Task_IU_Intake_Processing" name="IU Intake - Intake Processing">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="IU_INTAKE_OFFICER_GROUP" />
        <zeebe:formDefinition externalReference="iuIntakeProcessingForm" />
        <zeebe:userTask />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_to_IU_Processing</bpmn:incoming>
      <bpmn:outgoing>Flow_to_IU_Assignment</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:userTask id="Task_IU_Assignment" name="IU Assignment - Assignment">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="IU_MANAGER_GROUP" />
        <zeebe:formDefinition externalReference="iuAssignmentForm" />
        <zeebe:userTask />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_to_IU_Assignment</bpmn:incoming>
      <bpmn:outgoing>Flow_to_End_IU_Mgmt</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:endEvent id="EndEvent_IU_Management" name="Assigned to Investigation Team">
      <bpmn:incoming>Flow_to_End_IU_Mgmt</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows -->
    <bpmn:sequenceFlow id="Flow_to_IU_Intake_Assignment" sourceRef="StartEvent_IU_Management" targetRef="Task_IU_Intake_Assignment" />
    <bpmn:sequenceFlow id="Flow_to_IU_Processing" sourceRef="Task_IU_Intake_Assignment" targetRef="Task_IU_Intake_Processing" />
    <bpmn:sequenceFlow id="Flow_to_IU_Assignment" sourceRef="Task_IU_Intake_Processing" targetRef="Task_IU_Assignment" />
    <bpmn:sequenceFlow id="Flow_to_End_IU_Mgmt" sourceRef="Task_IU_Assignment" targetRef="EndEvent_IU_Management" />
    
  </bpmn:process>

  <!-- Process 4: Investigation Unit - Investigation Team -->
  <bpmn:process id="Process_IU_Investigation" name="Investigation Process" isExecutable="false">
    
    <bpmn:startEvent id="StartEvent_Investigation" name="Investigation Assignment Received">
      <bpmn:outgoing>Flow_to_Plan_Creation</bpmn:outgoing>
    </bpmn:startEvent>
    
    <bpmn:userTask id="Task_Investigation_Plan_Creation" name="IU Investigation - Investigation Plan Creation">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="INVESTIGATOR_GROUP" />
        <zeebe:formDefinition externalReference="investigationPlanCreationForm" />
        <zeebe:userTask />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_to_Plan_Creation</bpmn:incoming>
      <bpmn:incoming>Flow_Plan_Rejected</bpmn:incoming>
      <bpmn:outgoing>Flow_to_Plan_Review</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:userTask id="Task_Investigation_Plan_Review" name="IU Investigation - Investigation Plan Review">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="INVESTIGATION_MANAGER_GROUP" />
        <zeebe:formDefinition externalReference="investigationPlanReviewForm" />
        <zeebe:userTask />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_to_Plan_Review</bpmn:incoming>
      <bpmn:outgoing>Flow_to_Plan_Gateway</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:exclusiveGateway id="Gateway_Plan_Approved" name="Plan Approved?">
      <bpmn:incoming>Flow_to_Plan_Gateway</bpmn:incoming>
      <bpmn:outgoing>Flow_Plan_Approved</bpmn:outgoing>
      <bpmn:outgoing>Flow_Plan_Rejected</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:userTask id="Task_Active_Investigation" name="IU Investigation - Active Investigation">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="INVESTIGATOR_GROUP" />
        <zeebe:formDefinition externalReference="activeInvestigationForm" />
        <zeebe:userTask />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Plan_Approved</bpmn:incoming>
      <bpmn:outgoing>Flow_to_Finalization</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:userTask id="Task_Investigation_Finalization" name="IU Investigation - Finalization">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="INVESTIGATOR_GROUP" />
        <zeebe:formDefinition externalReference="investigationFinalizationForm" />
        <zeebe:userTask />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_to_Finalization</bpmn:incoming>
      <bpmn:outgoing>Flow_to_End_Investigation</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:endEvent id="EndEvent_Investigation" name="Investigation Complete">
      <bpmn:incoming>Flow_to_End_Investigation</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows -->
    <bpmn:sequenceFlow id="Flow_to_Plan_Creation" sourceRef="StartEvent_Investigation" targetRef="Task_Investigation_Plan_Creation" />
    <bpmn:sequenceFlow id="Flow_to_Plan_Review" sourceRef="Task_Investigation_Plan_Creation" targetRef="Task_Investigation_Plan_Review" />
    <bpmn:sequenceFlow id="Flow_to_Plan_Gateway" sourceRef="Task_Investigation_Plan_Review" targetRef="Gateway_Plan_Approved" />
    
    <bpmn:sequenceFlow id="Flow_Plan_Approved" name="Approved" sourceRef="Gateway_Plan_Approved" targetRef="Task_Active_Investigation">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=planApproved = true</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_Plan_Rejected" name="Rejected" sourceRef="Gateway_Plan_Approved" targetRef="Task_Investigation_Plan_Creation">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=planApproved = false</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_to_Finalization" sourceRef="Task_Active_Investigation" targetRef="Task_Investigation_Finalization" />
    <bpmn:sequenceFlow id="Flow_to_End_Investigation" sourceRef="Task_Investigation_Finalization" targetRef="EndEvent_Investigation" />
    
  </bpmn:process>

  <!-- Process 5: Ethics Office - Case Closure -->
  <bpmn:process id="Process_Ethics_Closure" name="Ethics Office Case Closure" isExecutable="false">
    
    <bpmn:startEvent id="StartEvent_Closure" name="Investigation Complete">
      <bpmn:outgoing>Flow_to_Close_EO</bpmn:outgoing>
    </bpmn:startEvent>
    
    <bpmn:userTask id="Task_Close_EO" name="Close EO - Ethics Office">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="DIRECTOR_GROUP" />
        <zeebe:formDefinition externalReference="closeEoForm" />
        <zeebe:userTask />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_to_Close_EO</bpmn:incoming>
      <bpmn:outgoing>Flow_to_Case_Closed</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:endEvent id="EndEvent_Case_Closed" name="Case Closed">
      <bpmn:incoming>Flow_to_Case_Closed</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows -->
    <bpmn:sequenceFlow id="Flow_to_Close_EO" sourceRef="StartEvent_Closure" targetRef="Task_Close_EO" />
    <bpmn:sequenceFlow id="Flow_to_Case_Closed" sourceRef="Task_Close_EO" targetRef="EndEvent_Case_Closed" />
    
  </bpmn:process>

  <!-- BPMN Diagram with 5 Pools Layout -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_5_Pools">
    <bpmndi:BPMNPlane id="BPMNPlane_5_Pools" bpmnElement="Collaboration_CMS_Process">
      
      <!-- Pool 1: Ethics Office - Intake & Classification -->
      <bpmndi:BPMNShape id="Participant_Ethics_Office_di" bpmnElement="Participant_Ethics_Office" isHorizontal="true">
        <dc:Bounds x="160" y="80" width="800" height="200" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      
      <!-- Pool 2: Specialized Departments -->
      <bpmndi:BPMNShape id="Participant_Specialized_Depts_di" bpmnElement="Participant_Specialized_Depts" isHorizontal="true">
        <dc:Bounds x="160" y="320" width="800" height="300" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      
      <!-- Pool 3: Investigation Unit - Management -->
      <bpmndi:BPMNShape id="Participant_IU_Management_di" bpmnElement="Participant_IU_Management" isHorizontal="true">
        <dc:Bounds x="160" y="660" width="800" height="200" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      
      <!-- Pool 4: Investigation Unit - Investigation Team -->
      <bpmndi:BPMNShape id="Participant_IU_Investigation_di" bpmnElement="Participant_IU_Investigation" isHorizontal="true">
        <dc:Bounds x="160" y="900" width="1200" height="200" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      
