<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" 
  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" 
  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" 
  xmlns:zeebe="http://camunda.org/schema/zeebe/1.0" 
  xmlns:di="http://www.omg.org/spec/DD/20100524/DI" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xmlns:modeler="http://camunda.org/schema/modeler/1.0" 
  id="Definitions_case_management" 
  targetNamespace="http://bpmn.io/schema/bpmn" 
  exporter="Camunda Modeler" 
  exporterVersion="5.35.0" 
  modeler:executionPlatform="Camunda Cloud" 
  modeler:executionPlatformVersion="8.5.0">

  <bpmn:process id="case-management-process" name="Case Management Process" isExecutable="true">
    
    <bpmn:startEvent id="StartEvent_CaseCreated" name="Case Created">
      <bpmn:outgoing>Flow_to_Intake</bpmn:outgoing>
    </bpmn:startEvent>
    
    <bpmn:userTask id="Task_EO_Intake" name="EO Intake">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="INTAKE_ANALYST" />
        <zeebe:formDefinition formKey="camunda-forms:bpmn:userTaskForm_1" />
        <zeebe:userTask />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_to_Intake</bpmn:incoming>
      <bpmn:outgoing>Flow_to_Classification</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:businessRuleTask id="Task_Classification" name="Classify Allegation">
      <bpmn:extensionElements>
        <zeebe:calledDecision decisionId="allegation-classification" resultVariable="classificationResult" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_to_Classification</bpmn:incoming>
      <bpmn:outgoing>Flow_to_Gateway</bpmn:outgoing>
    </bpmn:businessRuleTask>
    
    <bpmn:exclusiveGateway id="Gateway_Classification" name="Route by Classification">
      <bpmn:incoming>Flow_to_Gateway</bpmn:incoming>
      <bpmn:outgoing>Flow_to_HR</bpmn:outgoing>
      <bpmn:outgoing>Flow_to_Legal</bpmn:outgoing>
      <bpmn:outgoing>Flow_to_CSIS</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:userTask id="Task_HR_Assignment" name="HR Assignment">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="HR_SPECIALIST" />
        <zeebe:formDefinition formKey="camunda-forms:bpmn:userTaskForm_2" />
        <zeebe:userTask />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_to_HR</bpmn:incoming>
      <bpmn:outgoing>Flow_HR_to_Investigation</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:userTask id="Task_Legal_Assignment" name="Legal Assignment">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="LEGAL_COUNSEL" />
        <zeebe:formDefinition formKey="camunda-forms:bpmn:userTaskForm_3" />
        <zeebe:userTask />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_to_Legal</bpmn:incoming>
      <bpmn:outgoing>Flow_Legal_to_Investigation</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:userTask id="Task_CSIS_Assignment" name="CSIS Assignment">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="SECURITY_ANALYST" />
        <zeebe:formDefinition formKey="camunda-forms:bpmn:userTaskForm_4" />
        <zeebe:userTask />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_to_CSIS</bpmn:incoming>
      <bpmn:outgoing>Flow_CSIS_to_Investigation</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:exclusiveGateway id="Gateway_Converge" name="Converge to Investigation">
      <bpmn:incoming>Flow_HR_to_Investigation</bpmn:incoming>
      <bpmn:incoming>Flow_Legal_to_Investigation</bpmn:incoming>
      <bpmn:incoming>Flow_CSIS_to_Investigation</bpmn:incoming>
      <bpmn:outgoing>Flow_to_Investigation</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:userTask id="Task_Investigation" name="Investigation">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="INVESTIGATOR" />
        <zeebe:formDefinition formKey="camunda-forms:bpmn:userTaskForm_5" />
        <zeebe:userTask />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_to_Investigation</bpmn:incoming>
      <bpmn:outgoing>Flow_to_Closure</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:userTask id="Task_Case_Closure" name="Case Closure">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="DIRECTOR" />
        <zeebe:formDefinition formKey="camunda-forms:bpmn:userTaskForm_6" />
        <zeebe:userTask />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_to_Closure</bpmn:incoming>
      <bpmn:outgoing>Flow_to_End</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:endEvent id="EndEvent_CaseClosed" name="Case Closed">
      <bpmn:incoming>Flow_to_End</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows -->
    <bpmn:sequenceFlow id="Flow_to_Intake" sourceRef="StartEvent_CaseCreated" targetRef="Task_EO_Intake" />
    <bpmn:sequenceFlow id="Flow_to_Classification" sourceRef="Task_EO_Intake" targetRef="Task_Classification" />
    <bpmn:sequenceFlow id="Flow_to_Gateway" sourceRef="Task_Classification" targetRef="Gateway_Classification" />
    
    <bpmn:sequenceFlow id="Flow_to_HR" name="HR Cases" sourceRef="Gateway_Classification" targetRef="Task_HR_Assignment">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=classificationResult.classification = "HR"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_to_Legal" name="Legal Cases" sourceRef="Gateway_Classification" targetRef="Task_Legal_Assignment">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=classificationResult.classification = "LEGAL"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_to_CSIS" name="CSIS Cases" sourceRef="Gateway_Classification" targetRef="Task_CSIS_Assignment">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=classificationResult.classification = "CSIS"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_HR_to_Investigation" sourceRef="Task_HR_Assignment" targetRef="Gateway_Converge" />
    <bpmn:sequenceFlow id="Flow_Legal_to_Investigation" sourceRef="Task_Legal_Assignment" targetRef="Gateway_Converge" />
    <bpmn:sequenceFlow id="Flow_CSIS_to_Investigation" sourceRef="Task_CSIS_Assignment" targetRef="Gateway_Converge" />
    <bpmn:sequenceFlow id="Flow_to_Investigation" sourceRef="Gateway_Converge" targetRef="Task_Investigation" />
    <bpmn:sequenceFlow id="Flow_to_Closure" sourceRef="Task_Investigation" targetRef="Task_Case_Closure" />
    <bpmn:sequenceFlow id="Flow_to_End" sourceRef="Task_Case_Closure" targetRef="EndEvent_CaseClosed" />
    
  </bpmn:process>

  <!-- BPMN Diagram -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="case-management-process">
      
      <bpmndi:BPMNShape id="StartEvent_CaseCreated_di" bpmnElement="StartEvent_CaseCreated">
        <dc:Bounds x="152" y="102" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="137" y="145" width="66" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_EO_Intake_di" bpmnElement="Task_EO_Intake">
        <dc:Bounds x="240" y="80" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Classification_di" bpmnElement="Task_Classification">
        <dc:Bounds x="390" y="80" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Gateway_Classification_di" bpmnElement="Gateway_Classification" isMarkerVisible="true">
        <dc:Bounds x="545" y="95" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="525" y="65" width="90" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_HR_Assignment_di" bpmnElement="Task_HR_Assignment">
        <dc:Bounds x="650" y="30" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Legal_Assignment_di" bpmnElement="Task_Legal_Assignment">
        <dc:Bounds x="650" y="120" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_CSIS_Assignment_di" bpmnElement="Task_CSIS_Assignment">
        <dc:Bounds x="650" y="210" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Gateway_Converge_di" bpmnElement="Gateway_Converge" isMarkerVisible="true">
        <dc:Bounds x="805" y="135" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="790" y="105" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Investigation_di" bpmnElement="Task_Investigation">
        <dc:Bounds x="900" y="120" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Case_Closure_di" bpmnElement="Task_Case_Closure">
        <dc:Bounds x="1050" y="120" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="EndEvent_CaseClosed_di" bpmnElement="EndEvent_CaseClosed">
        <dc:Bounds x="1202" y="142" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1189" y="185" width="63" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Sequence Flow Edges -->
      <bpmndi:BPMNEdge id="Flow_to_Intake_di" bpmnElement="Flow_to_Intake">
        <di:waypoint x="188" y="120" />
        <di:waypoint x="240" y="120" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_to_Classification_di" bpmnElement="Flow_to_Classification">
        <di:waypoint x="340" y="120" />
        <di:waypoint x="390" y="120" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_to_Gateway_di" bpmnElement="Flow_to_Gateway">
        <di:waypoint x="490" y="120" />
        <di:waypoint x="545" y="120" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_to_HR_di" bpmnElement="Flow_to_HR">
        <di:waypoint x="570" y="95" />
        <di:waypoint x="570" y="70" />
        <di:waypoint x="650" y="70" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="583" y="52" width="51" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_to_Legal_di" bpmnElement="Flow_to_Legal">
        <di:waypoint x="595" y="120" />
        <di:waypoint x="623" y="120" />
        <di:waypoint x="623" y="160" />
        <di:waypoint x="650" y="160" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="595" y="142" width="62" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_to_CSIS_di" bpmnElement="Flow_to_CSIS">
        <di:waypoint x="570" y="145" />
        <di:waypoint x="570" y="250" />
        <di:waypoint x="650" y="250" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="583" y="232" width="60" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_HR_to_Investigation_di" bpmnElement="Flow_HR_to_Investigation">
        <di:waypoint x="750" y="70" />
        <di:waypoint x="830" y="70" />
        <di:waypoint x="830" y="135" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Legal_to_Investigation_di" bpmnElement="Flow_Legal_to_Investigation">
        <di:waypoint x="750" y="160" />
        <di:waypoint x="805" y="160" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_CSIS_to_Investigation_di" bpmnElement="Flow_CSIS_to_Investigation">
        <di:waypoint x="750" y="250" />
        <di:waypoint x="830" y="250" />
        <di:waypoint x="830" y="185" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_to_Investigation_di" bpmnElement="Flow_to_Investigation">
        <di:waypoint x="855" y="160" />
        <di:waypoint x="900" y="160" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_to_Closure_di" bpmnElement="Flow_to_Closure">
        <di:waypoint x="1000" y="160" />
        <di:waypoint x="1050" y="160" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_to_End_di" bpmnElement="Flow_to_End">
        <di:waypoint x="1150" y="160" />
        <di:waypoint x="1202" y="160" />
      </bpmndi:BPMNEdge>
      
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>

</bpmn:definitions>